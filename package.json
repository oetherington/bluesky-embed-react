{
	"name": "bluesky-embed-react",
	"version": "0.0.9",
	"description": "React component to embed bluesky posts, profiles and feeds",
	"keywords": [
		"react",
		"bluesky",
		"bsky",
		"embed",
		"social",
		"media"
	],
	"homepage": "https://github.com/oetherington/bluesky-embed-react#readme",
	"bugs": {
		"url": "https://github.com/oetherington/bluesky-embed-react/issues"
	},
	"repository": "github:oetherington/bluesky-embed-react",
	"author": {
		"name": "Ollie Etherington",
		"url": "https://github.com/oetherington"
	},
	"license": "MIT",
	"scripts": {
		"prepare": "husky",
		"lint": "eslint",
		"tsc": "tsc --skipLibCheck --noEmit",
		"build": "tsup",
		"check-format": "prettier --check .",
		"format": "prettier . --write --log-level warn",
		"install-browsers": "playwright install --with-deps",
		"test": "test-storybook --browsers chromium,firefox,webkit --coverage",
		"storybook": "storybook dev -p 6006 --no-open",
		"build-storybook": "storybook build"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"/dist"
	],
	"peerDependencies": {
		"react": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
		"react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.15.0",
		"@storybook/addon-coverage": "^1.0.4",
		"@storybook/addon-essentials": "^8.4.5",
		"@storybook/manager-api": "^8.4.5",
		"@storybook/react-vite": "^8.4.5",
		"@storybook/test-runner": "0.23.0",
		"@storybook/theming": "^8.4.5",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.4",
		"eslint": "^9.15.0",
		"husky": "^9.1.7",
		"prettier": "^3.3.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"storybook": "^8.4.5",
		"storybook-dark-mode": "^4.0.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.15.0"
	},
	"dependencies": {
		"@atproto/api": "^0.15.8",
		"hls.js": "^1.5.17"
	}
}
